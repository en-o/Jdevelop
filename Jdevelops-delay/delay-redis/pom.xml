            <groupId>com.alibaba</groupId>
            <artifactId>fastjson</artifactId>
            <!-- 引用时自己依赖吧，必须要 -->
            <optional>true</optional>
        </dependency>
        <dependency>
            <groupId>cn.jdevelops</groupId>
            <artifactId>caches-redis-core</artifactId>
            <version>${jdevelops.last.version}</version>
            <!-- 引用时自己依赖吧，必须要 -->
